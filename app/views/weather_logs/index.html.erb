<h1>Listing weather_logs</h1>

<table>
  <thead>
    <tr>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Datatime</th>
      <th>Air temperature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weather_logs.each do |weather_log| %>
      <tr>
        <td><%= weather_log.latitude %></td>
        <td><%= weather_log.longitude %></td>
        <td><%= weather_log.datatime %></td>
        <td><%= weather_log.air_temperature %></td>
        <td><%= link_to 'Show', weather_log %></td>
        <td><%= link_to 'Edit', edit_weather_log_path(weather_log) %></td>
        <td><%= link_to 'Destroy', weather_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weather log', new_weather_log_path %>
